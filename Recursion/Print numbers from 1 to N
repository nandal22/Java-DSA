import java.util.*;
public class Main
{
    public static void print1toN(int n){
        if(n<0){     // Base Condition
            return;
        }
        print1toN(n-1);     // Recursive Function
        System.out.println(n);
    }
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n =  sc.nextInt();
		print1toN(n);
	}
}
