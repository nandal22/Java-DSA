import java.util.*;
public class Main
{
    public static void printNto1(int n){
        if(n==0){     // Base Condition
            return;
        }
        System.out.println(n);
        printNto1(n-1);     // Recursive Function
    }
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n =  sc.nextInt();
		printNto1(n);
	}
}
